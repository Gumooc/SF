<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.run.dao.CommentDao">
	<insert id = "insert" parameterType="com.run.entity.Comment">
		insert into comment(uid,bid,cid,lst) values(#{uid},#{bid},#{cid},#{lst});
	</insert>
	<delete id = "delete" parameterType="com.run.entity.Comment">
		delete from comment where cid=#{cid};
	</delete>
</mapper>